{
  "name": "brokeranalysis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "build": "vite build",
    "build:skip-lint": "vite build",
    "build:prod": "npx vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache",
    "compile-brokers": "tsx scripts/compileBrokerData.ts",
    "extract-brokers": "tsx scripts/extractBrokerData.ts",
    "test:assets": "node scripts/test-assets.js",
    "test:assets:dev": "npm run test:assets",
    "verify:build": "npm run build && npm run test:assets",
    "client:dev": "vite",
    "server:dev": "nodemon",
    "ai:generate-brokers": "tsx src/ai-content-generator/scripts/cli.ts generate-brokers",
    "ai:check-env": "tsx src/ai-content-generator/scripts/cli.ts check-env",
    "ai:list-brokers": "tsx src/ai-content-generator/scripts/cli.ts list-brokers",
    "ai:cli": "tsx src/ai-content-generator/scripts/cli.ts",
    "mcp:build": "cd mcp-servers/src/puppeteer && npm run build",
    "mcp:start": "cd mcp-servers/src/puppeteer && npm start",
    "mcp:dev": "cd mcp-servers/src/puppeteer && npm run dev",
    "mcp:install": "cd mcp-servers/src/puppeteer && npm install"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/sharp": "^0.31.1",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "puppeteer": "^24.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^6.26.2",
    "sharp": "^0.34.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.6.3",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.536.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.1.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
