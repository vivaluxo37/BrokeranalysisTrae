# BrokerChooser Crawler Environment Variables
# Copy this file to .env and fill in your actual values

# Bright Data Web Unlocker Direct API Configuration
BRIGHTDATA_API_KEY=your_brightdata_api_key_here
BRIGHTDATA_ZONE=your_brightdata_zone_here

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Crawler Configuration (Optional)
CRAWL_CONCURRENCY=6
CRAWL_TIMEOUT=120000
CRAWL_RETRIES=3
CRAWL_DELAY=1000

# Logging Configuration (Optional)
LOG_LEVEL=info
LOG_FILE=crawler.log

# Database Configuration (Optional)
DB_BATCH_SIZE=50
DB_TIMEOUT=30000

# Resume Configuration (Optional)
RESUME_ENABLED=true
FORCE_RECRAWL=false